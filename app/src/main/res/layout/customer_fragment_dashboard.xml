<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/holo_red_light">




        <TextView
            android:id="@+id/dashboardTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="MAIN MENU"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp"
            android:textSize="23sp"
            android:textStyle="bold"

            />

        <androidx.gridlayout.widget.GridLayout
            android:layout_below="@+id/dashboardTitle"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            app:columnCount="2"
            app:rowCount="3">

                <androidx.cardview.widget.CardView
                    android:id="@+id/plumberCard"
                    app:cardCornerRadius="30dp"
                    android:layout_width="173dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:paddingTop="20dp"
                    app:cardElevation="10dp"
                    app:layout_column="0"
                    app:layout_gravity="fill"
                    app:layout_row="0">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="15dp"
                            android:orientation="vertical"

                            >

                                <ImageView
                                    android:id="@+id/box1"
                                    android:layout_width="100dp"
                                    android:layout_height="100dp"
                                    app:srcCompat="@mipmap/ic_app_logo" />

                                <TextView
                                    android:id="@+id/box2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Plumber"
                                    android:textSize="20sp"
                                    android:textColor="#000000" />


                        </LinearLayout>


                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/masonryCard"
                    android:layout_width="173dp"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="30dp"
                    android:layout_margin="10dp"
                    android:paddingTop="20dp"
                    app:cardElevation="10dp"
                    app:layout_column="0"
                    app:layout_gravity="fill"
                    app:layout_row="1">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="15dp"
                            android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/messonryCard"
                                    android:layout_width="100dp"
                                    android:layout_height="100dp"
                                    app:srcCompat="@mipmap/ic_masonry_logo" />

                                <TextView
                                    android:id="@+id/textView10"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Masonry"
                                    android:textSize="20sp"
                                    android:textColor="#000000" />


                        </LinearLayout>

                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:id="@+id/gardenerCard"
                    app:cardCornerRadius="30dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:paddingTop="20dp"
                    app:cardElevation="10dp"
                    app:layout_column="1"
                    app:layout_gravity="fill"
                    app:layout_row="1">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="15dp"
                            android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/imaw6"
                                    android:layout_width="100dp"
                                    android:layout_height="100dp"
                                    app:srcCompat="@mipmap/ic_gardener_logo" />

                                <TextView
                                    android:id="@+id/text10"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Gardener"
                                    android:textSize="20sp"
                                    android:textColor="#000000" />

                        </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/electricianCard"
                    app:cardCornerRadius="30dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:paddingTop="20dp"
                    app:cardElevation="10dp"
                    app:layout_column="1"
                    app:layout_gravity="fill"
                    app:layout_row="0">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="15dp"
                            android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/imw6"
                                    android:layout_width="100dp"
                                    android:layout_height="100dp"
                                    app:srcCompat="@mipmap/ic_electrician_logo" />

                                <TextView
                                    android:id="@+id/electricrd"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Electrician"
                                    android:textSize="20sp"
                                    android:textColor="#000000" />

                        </LinearLayout>
                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:id="@+id/carpentryCard"
                    app:cardCornerRadius="30dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:paddingTop="20dp"
                    app:cardElevation="10dp"
                    app:layout_column="1"
                    app:layout_gravity="fill"
                    app:layout_row="2">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="15dp"
                            android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/imdw6"
                                    android:layout_width="100dp"
                                    android:layout_height="100dp"
                                    app:srcCompat="@mipmap/ic_carpentry_logo" />

                                <TextView
                                    android:id="@+id/dsdds"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textSize="20sp"
                                    android:text="Carpenter" />
                        </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/computerTechnicianCard"
                    android:layout_width="173dp"
                    app:cardCornerRadius="30dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:paddingTop="20dp"
                    app:cardElevation="10dp"
                    app:layout_column="0"
                    app:layout_gravity="fill"
                    app:layout_row="2">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="15dp"
                            android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/imdsw6"
                                    android:layout_width="100dp"
                                    android:layout_height="100dp"
                                    app:srcCompat="@mipmap/ic_computer_repair_logo" />

                                <TextView
                                    android:id="@+id/sassss"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Computer Technician"
                                    android:textSize="20sp"
                                    android:textColor="#000000" />


                        </LinearLayout>
                </androidx.cardview.widget.CardView>


        </androidx.gridlayout.widget.GridLayout>
</RelativeLayout>